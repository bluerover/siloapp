        //==ANIMATE bin FUNCTION
        var animateBin = function(parametric,percentValue,dy,sideColor,topColor){
           d3.select("#sign")
            .transition()
            .duration(DURATION)
            .ease("cubic")
            .attr('y',(175+dy)+"")        
            var p = d3.select("#parametric")
            .data(parametric)
            .text(function(d){return d;})
            .style("fill-opacity", 1e-6)      
            .transition()
            .duration(DURATION)
            .ease("cubic")
            .attr('y',(125+dy)+"")        
            .style("fill-opacity", 1);
            
          d3.select("#percent")
            .data([percentValue])
            .transition()
            .duration(DURATION)
            .ease("cubic")
            .attr('y',(175+dy)+"")
            .tween("text", function(d,i,a){
              var i = d3.interpolate(this.textContent,d);
              return function(t){
                this.textContent = Math.round(i(t));
              }
            });
          
          g.select('rect')
            .transition()
            .duration(DURATION)
            .ease("cubic")
            .attr('y',(175+dy)+"")
            .attr('height',(115-dy)+"")
            .style("fill", sideColor)
            .style("stroke",sideColor) ;
              
          g.select('#bot-fluid')
            .transition()
            .duration(DURATION)
            .ease("cubic")
            .style("fill", sideColor)
            .style("stroke",sideColor) ;
          
          g.select('#top-fluid')
            .transition() 
            .duration(DURATION)
            .ease("cubic")
            .attrTween("transform", function(d,i,a) { 
              return d3.interpolateString(a, "translate(0,"+dy+")"); })
            .style("fill", topColor)
            .style("stroke",topColor) ;
        }
        
        function getUnique(array) {
          var u = {}, a = [];
          for(var i = 0, l = array.length; i < l; ++i){
            if(u.hasOwnProperty(array[i])) {
              continue;
            }
            a.push(array[i]);
            u[array[i]] = 1;
          }
          return a;
        }

        var levels = [<%= bin.level_1 %>,<%= bin.level_2 %>,<%= bin.level_3 %>,<%= bin.level_4 %>];
        
        currentLevel = 100;
        <% if (!(bindata[0] == null)) { %>
          currentLevel = <%= bindata[0].level %> ;
        <% } %>
        
        var colors = [["#00ff00","#56ff56"],["#ccff66","#FFFF66"],["#ffff00","#ffff5f"],["#ff7f00","#ffaa56"],["#ff0000","#ff5656"]];
        var animateState = function (level) {
          if(level >= <%= bin.level_1 %>) {//"above level 1"
            animateBin(['above ' + <%= bin.level_1 %>],level,100-level,colors[0][0],colors[0][1]);
          } else if(level <= <%= bin.level_4 %>){//"below level 4"
            animateBin(['below ' + <%= bin.level_4 %>],level,100-level,colors[4][0],colors[4][1]);
          } else {
            levels = getUnique(levels);//must account for something like 80,80,40,40 and 80,50,50,20
            for(var i = 0, l = levels.length-1; i < l; ++i) {
              if(level <= levels[i] && level >= levels[i+1]) {
                animateBin(['between ' + levels[i] + ' and ' + levels[i+1]],level,100-level,colors[i+1][0], colors[i+1][1]);
              }
            }
          } 
        }

        animateState(currentLevel);
