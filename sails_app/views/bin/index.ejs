        DURATION = 500;
        //======DRAW bin ====
        var elem = d3.select('#bin');
        var g = elem.select('svg g');
        //default state
        var handle = function(data){  
          try{
            state = data;
            if(!state.hasOwnProperty(21014) || !state.hasOwnProperty(21021)){
              return
            }
            //update time
            var timestamp = state[21014]['timestamp'] >= state[21021]['timestamp']  ?
              state[21014]['timestamp'] : state[21021]['timestamp'] ;
            $('#time').html(moment.unix(timestamp).format('MMMM Do YYYY, h:mm:ss a')); 
            
            //21014-->30%
            //21021-->20%
            if(state[21014]['statusCode'] === 62730 ){ //logically bot should still be closed 
              animateState(0);
            }else if(state[21014]['statusCode'] === 62731 && state[21021]['statusCode'] === 62730){
              animateState(1);
            }else if (state[21014]['statusCode'] === 62731 && state[21021]['statusCode'] === 62731){
              animateState(2);
            }
          }
          catch (err){
            
          }
        };
