<div class='col-lg-6 col-md-8 col-xs-12'>
    <div class='panel panel-default'>
        <div class="widget large-height" visualization="<%= widget.widget.name %>" data-filter="<%= widget.data_filter %>" data-id="<%= widget.id %>" data-options="<%= widget.options %>">
            <div class="widget-title" class="row">
                <%= widget.display_name %>
            </div>
            <div id="floorplan-content-<%= widget.id %>" class="content">
                <div class="loading-div">
                    <img id="loader-<%= widget.id %>" style="position: relative; top: calc(50% - 32px + 15px);" src="/images/loading.gif">
                </div>
                <div id="floorplan-svg" class="row">
                </div>
            </div>
        </div>
    </div>
</div>